{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://example.com/minimum",
  "type": "object",
  "properties": {
    "myNumber": {
      "type": "number",
      "minimum": 1.2
    },
    "myInteger": {
      "type": "integer",
      "minimum": 2
    },
    "myNullableNumber": {
      "type": "number",
      "minimum": 1.2
    },
    "myNullableInteger": {
      "type": "integer",
      "minimum": 2
    }
  },
  "required": [
    "myNumber",
    "myInteger"
  ]
}
