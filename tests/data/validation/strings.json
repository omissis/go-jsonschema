{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://example.com/stringValidation",
  "type": "object",
  "required": [
    "country_code"
  ],
  "properties": {
    "country_code": {
      "type": "string",
      "pattern": "^[A-Z]{2}$",
      "minLength": 2,
      "maxLength": 2
    }
  }
}
